procedure WinOrLose(s: String);
	var
	    i, l: Integer;
	    us: String;

	begin
	    i := 1;
	    l := Length(s);
	    while i <= l do
	    begin
	        if (ord(s[i]) < 128) then
	        begin
	            write(s[i]);
	        end
	        else
	        begin
	            us := s[i] + s[i+1];
	            write(us);
	            
	            i := i + 1;
	        end;

	        i := i + 1;
	        delay(50);        
	    end;
	    writeln;
	end;	

